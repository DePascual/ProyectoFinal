<div class="row" ng-controller="trackCtrl">
    <!--COLUMNA IZQUIERDA-->
    <div class="col-lg-4">

        <div id="busqueda">
            <form class="form-inline">
                <div class="form-group">
                    <div class="input-group">
                        <input id="textBusqueda" name="textBusqueda" type="text" class="form-control" placeholder="Search" />
                    </div>
                </div>
                <button type="button" class="btn btn-default" ng-click="buscar()">Search</button>
            </form>
        </div>
        <br />
        <hr />

        <div id="subMenu">
            <div id="preMade">
                <a id="link_preMadeWorkouts" ng-click="showDiv('preMadeWorkouts', $event)" show="false" style="font-size:medium; font-weight:bold">Pre-Made workouts  <i class="fa fa-chevron-up" aria-hidden="true"></i></a>
                <br />

                <div id="preMadeWorkouts" style="display:none">
                    <div ng-repeat="preW in preMadeWorkouts">
                        <a style="font-size:medium" ng-click="descripcionEnPartial($event)" data="{{preW._id}}">{{preW.Nombre}}</a>
                        <span>{{preW.Puntos}}</span>
                        @*<div ng-repeat="track in preW.Tracks">
                                <p>{{track.Nombre}}</p>
                            </div>*@
                    </div>
                </div>
            </div>
            <div id="recent">
                <a id="link_recentWorkouts" ref="showDiv" ng-click="showDiv('recentWorkouts', $event)" show="false" style="font-size:medium; font-weight:bold">Recent workouts  <i class="fa fa-chevron-up" aria-hidden="true"></i></a>
                <br />
                <div id="recentWorkouts" style="display:none">
                    fldsfhglsdkhja
                </div>
            </div>
        </div>
        <div id="all"></div>
    </div>

    <!--COLUMNA DERECHA-->
    <div class="col-lg-8" id="vistaParcial">
        @*/<div id="grid" style="width:940px; height:300px;" ui-grid="{ data: Tracks,  columnDefs: [{ name: '_id', visible: false }] }" ></div>*@
    </div>

</div>

